<h1>Astronauts</h1>
<h2>Average Age: <%= @average_age %></h2>
<% @astronauts.each do |astronaut| %>
  <ul>
    <li><%= astronaut.name %></li>
    <li><%= astronaut.age %></li>
    <li><%= astronaut.job %></li>
    <% astronaut.astronaut_missions.each do |astro_mission| %>
      <li><%= astro_mission.space_mission.title %></li>
    <% end %>
  </ul>
<% end %>
